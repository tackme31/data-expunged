{
  "name": "[DATA EXPUNGED]",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "A chrome extension to hide text that contains specified words.",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["/static/js/content_script.js"]
  }],
  "options_ui": {
    "page": "index.html",
    "open_in_tab": true
  }
}